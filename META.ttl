@prefix cpan:  <http://purl.org/NET/cpan-uri/person/> .
@prefix cpant: <http://purl.org/NET/cpan-uri/terms#> .
@prefix dc:    <http://purl.org/dc/terms/> .
@prefix dcs:   <http://ontologi.es/doap-changeset#> .
@prefix dist:  <http://purl.org/NET/cpan-uri/dist/TOBYINK-Pod-HTML/> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix nfo:   <http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

dist:project
	a                 doap:Project;
	doap:name         "TOBYINK-Pod-HTML";
	cpant:install_script [
		a nfo:FileDataObject;
		nfo:fileName "bin/pod2html-tobyink";
	];
	cpant:perl_version_from [
		a nfo:FileDataObject;
		a nfo:SourceCode;
		rdfs:label "TOBYINK::Pod::HTML";
		nfo:fileName "lib/TOBYINK/Pod/HTML.pm";
		nfo:programmingLanguage "Perl";
	];
	cpant:readme_from [
		a nfo:FileDataObject;
		a nfo:SourceCode;
		rdfs:label "TOBYINK::Pod::HTML";
		nfo:fileName "lib/TOBYINK/Pod/HTML.pm";
		nfo:programmingLanguage "Perl";
	];
	cpant:recommends  "HTML::HTML5::Writer 0.201"^^cpant:dsWithVersion;
	cpant:recommends  "PPI::Document"^^cpant:dsWithoutVersion;
	cpant:recommends  "PPI::HTML"^^cpant:dsWithoutVersion;
	cpant:recommends  "Syntax::Highlight::Engine::Kate"^^cpant:dsWithoutVersion;
	cpant:recommends  "Syntax::Highlight::RDF"^^cpant:dsWithoutVersion;
	cpant:recommends  "XML::LibXML::PrettyPrint"^^cpant:dsWithoutVersion;
	cpant:requires    "HTML::HTML5::Parser 0.208"^^cpant:dsWithVersion;
	cpant:requires    "Moo 1.000008"^^cpant:dsWithVersion;
	cpant:requires    "XML::LibXML::QuerySelector 0.003"^^cpant:dsWithVersion;
	cpant:version_from [
		a nfo:FileDataObject;
		a nfo:SourceCode;
		rdfs:label "TOBYINK::Pod::HTML";
		nfo:fileName "lib/TOBYINK/Pod/HTML.pm";
		nfo:programmingLanguage "Perl";
	];
	dc:contributor    cpan:tobyink;
	doap:bug-database <http://rt.cpan.org/Dist/Display.html?Queue=TOBYINK-Pod-HTML>;
	doap:created      "2013-01-18"^^xsd:date;
	doap:developer    cpan:tobyink;
	doap:download-page <https://metacpan.org/release/TOBYINK-Pod-HTML>;
	doap:homepage     <https://metacpan.org/release/TOBYINK-Pod-HTML>;
	doap:license      <http://dev.perl.org/licenses/>;
	doap:maintainer   cpan:tobyink;
	doap:programming-language "Perl";
	doap:release      dist:v_0-001;
	doap:release      dist:v_0-002;
	doap:release      dist:v_0-003;
	doap:repository   [
		a doap:HgRepository;
		doap:browse <https://bitbucket.org/tobyink/p5-tobyink-pod-html>;
	];
	doap:shortdesc    "convert Pod to HTML like TOBYINK".

dist:v_0-001
	a                 doap:Version;
	rdfs:label        "Initial release";
	dc:identifier     "TOBYINK-Pod-HTML-0.001"^^xsd:string;
	dc:issued         "2013-02-26"^^xsd:date;
	dcs:released-by   cpan:tobyink;
	doap:file-release <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/TOBYINK-Pod-HTML-0.001.tar.gz>;
	doap:revision     "0.001"^^xsd:string.

dist:v_0-002
	a                 doap:Version;
	dc:identifier     "TOBYINK-Pod-HTML-0.002"^^xsd:string;
	dc:issued         "2013-03-21"^^xsd:date;
	dcs:changeset     [
		dcs:item [
			a dcs:Addition;
			rdfs:label "Allow non-Perl languages to be syntax highlighted too.";
		];
	];
	dcs:released-by   cpan:tobyink;
	doap:file-release <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/TOBYINK-Pod-HTML-0.002.tar.gz>;
	doap:revision     "0.002"^^xsd:string.

dist:v_0-003
	a                 doap:Version;
	dc:identifier     "TOBYINK-Pod-HTML-0.003"^^xsd:string;
	dc:issued         "2013-04-10"^^xsd:date;
	dcs:changeset     [
		dcs:item [
			a dcs:Addition;
			rdfs:label "Syntax highlighting for Semantic Web related languages using Syntax::Highlight::RDF.";
		];
		dcs:item [
			a dcs:Bugfix;
			rdfs:label "Switching syntax highlight language in documents that have no NAME section was broken due to Pod::Simple::PullParser's look-ahead feature. This should now work.";
		];
	];
	dcs:released-by   cpan:tobyink;
	doap:file-release <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/TOBYINK-Pod-HTML-0.003.tar.gz>;
	doap:revision     "0.003"^^xsd:string.

cpan:tobyink
	a                 foaf:Person;
	foaf:name         "Toby Inkster";
	foaf:mbox         <mailto:tobyink@cpan.org>;
	foaf:nick         "TOBYINK";
	foaf:page         <https://metacpan.org/author/TOBYINK>.

